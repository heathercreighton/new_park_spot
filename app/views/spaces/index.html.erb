<p id="notice"><%= notice %></p>

<h1>Listing Spaces</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Address</th>
      <th>Lat</th>
      <th>Long</th>
      <th>Open</th>
      <th>Start</th>
      <th>End</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @spaces.each do |space| %>
      <tr>
        <td><%= space.user_id %></td>
        <td><%= space.address %></td>
        <td><%= space.lat %></td>
        <td><%= space.long %></td>
        <td><%= space.open %></td>
        <td><%= space.start %></td>
        <td><%= space.end %></td>
        <td><%= space.price %></td>
        <td><%= link_to 'Show', space %></td>
        <td><%= link_to 'Edit', edit_space_path(space) %></td>
        <td><%= link_to 'Destroy', space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Space', new_space_path %>
